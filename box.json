{
    "name":"CommandBox Semantic Release",
    "version":"2.0.1",
    "author":"Eric Peterson",
    "homepage":"https://github.com/commandbox-modules/commandbox-semantic-release",
    "documentation":"https://github.com/commandbox-modules/commandbox-semantic-release",
    "location":"commandbox-modules/commandbox-semantic-release#v2.0.1",
    "scripts":{
        "postVersion":"package set location='commandbox-modules/commandbox-semantic-release#v`package version`'",
        "onRelease":"publish",
        "postPublish":"!git push && !git push --tags"
    },
    "repository":{
        "type":"git",
        "URL":"https://github.com/commandbox-modules/commandbox-semantic-release"
    },
    "slug":"commandbox-semantic-release",
    "shortDescription":"Automated releases for CFML applications",
    "keywords":[
        "release",
        "forgebox",
        "semantic-versioning",
        "automation"
    ],
    "private":false,
    "type":"commandbox-modules",
    "ignore":[
        "**/.*",
        "test",
        "tests"
    ],
    "dependencies":{
        "semver":"^1.1.3"
    },
    "installPaths":{
        "semver":"modules/semver"
    }
}